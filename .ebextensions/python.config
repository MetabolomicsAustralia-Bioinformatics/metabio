option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "metabio.settings"
    "PYTHONPATH": "/opt/python/current/app/metabio:$PYTHONPATH"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: metabio/metabio/wsgi.py
    NumProcesses: 3
    NumThreads: 20
  "aws:elasticbeanstalk:container:python:staticfiles":
    "/static/": "www/static/"